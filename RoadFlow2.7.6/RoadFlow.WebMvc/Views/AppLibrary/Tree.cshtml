@{
    ViewBag.Title = "Tree";

    string rootid = new RoadFlow.Platform.Dictionary().GetIDByCode("AppLibraryTypes").ToString();
}

<form id="form1" runat="server">
    <div id="tree"></div>
</form>
<script type="text/javascript">
    var AppID = '@Request.QueryString["appid"]';
    var TabID = '@Request.QueryString["tabid"]';
    var rootID = '@(new RoadFlow.Platform.Dictionary().GetIDByCode("AppLibraryTypes").ToString())';
    var roadTree = null;
   
    $(function ()
    {
        roadTree = new RoadUI.Tree({ id: "tree", path: "../Dict/Tree1?ischild=1&root=" + rootID, refreshpath: "../Dict/TreeRefresh", onclick: openUrl });
    });
    function openUrl(json)
    {
        parent.frames[1].location = "List?typeid=" + json.id + "&appid=" + AppID + "&tabid=" + TabID;
    }
    function reLoad(id)
    {
        roadTree.refresh(id);
    }
</script>