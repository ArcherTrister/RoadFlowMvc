@{
    ViewBag.Title = "Tree";
    Layout = "~/Views/Shared/_Layout.cshtml";
    Page.CheckApp = false;
}
<form id="form1" runat="server">
    <div id="tree"></div>
</form>
<script type="text/javascript">
    var AppID = '@Request.QueryString["appid"]';
    var isselect = '@Request.QueryString["isselect"]';
    var roadTree = null;
    $(function ()
    {
        roadTree = new RoadUI.Tree({ id: "tree", path: "Tree1", refreshpath: "TreeRefresh", onclick: openUrl });
    });
    function openUrl(json)
    {
        parent.frames[1].location = "List?id=" + json.id + "&appid=" + AppID + "&isselect=" + isselect;
    }
    function reLoad(id)
    {
        roadTree.refresh(id);
    }
</script>
